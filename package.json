{
    "name": "tree-sitter-tolk",
    "version": "0.4.5",
    "description": "A tree-sitter grammar for the Tolk assembly programming language",
    "repository": "https://github.com/ton-blockchain/tree-sitter-tolk",
    "license": "MIT",
    "author": "TON Blockchain",
    "main": "bindings/node",
    "keywords": [
        "incremental",
        "parsing",
        "tree-sitter",
        "tolk",
        "parser",
        "lexer"
    ],
    "scripts": {
        "test": "tree-sitter test",
        "test:update": "tree-sitter test -u",
        "parse": "tree-sitter parse  main.tolk -D --open-log",
        "play": "yarn run grammar:wasm && tree-sitter playground",
        "grammar:wasm": "tree-sitter generate && tree-sitter build --wasm"
    },
    "dependencies": {
        "node-addon-api": "^8.4.0",
        "node-gyp-build": "^4.8.4"
    },
    "peerDependencies": {
        "tree-sitter": "^0.21.1"
    },
    "peerDependenciesMeta": {
        "tree_sitter": {
            "optional": true
        }
    },
    "devDependencies": {
        "prebuildify": "^6.0.0",
        "prettier": "^3.2.5",
        "tree-sitter-cli": "^0.25.0"
    },
    "tree-sitter": [
        {
            "scope": "source.tolk",
            "file-types": [
                "tolk"
            ],
            "highlights": "queries/highlights.scm"
        }
    ],
    "packageManager": "yarn@4.6.0"
}
